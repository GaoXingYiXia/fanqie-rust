{
  "buildCommand": "cargo build --release --target x86_64-unknown-linux-gnu",
  "installCommand": "cargo fetch",
  "outputDirectory": "target/x86_64-unknown-linux-gnu/release",
  "functions": {
    "api/**/*.rs": {
      "runtime": "vercel-rust@4.0.9",
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/api/:path*"
    }
  ]
}